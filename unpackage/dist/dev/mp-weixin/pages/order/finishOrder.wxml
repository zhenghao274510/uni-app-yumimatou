<view class="content-warpe"><block wx:if="{{addressData.addressid!=''}}"><view><view class="address-tit">收货人信息</view><view data-event-opts="{{[['tap',[['choseAddress',['$event']]]]]}}" class="address-section" bindtap="__e"><view class="order-content"><view class="cen"><view class="top uni-flex"><view class="mobile"><text>{{addressData.name}}</text><text style="margin-left:15px;">{{addressData.phone}}</text></view></view><text class="cell-tit">{{addressData.address+addressData.addressdetail}}</text></view><view class="uni-icon uni-icon-arrowright"></view></view></view></view></block><block wx:else><view data-event-opts="{{[['tap',[['choseAddress',['$event']]]]]}}" class="noAddress" bindtap="__e"><view class="mobile address-tit">选择您的收货地址</view><view class="uni-icon uni-icon-arrowright"></view></view></block><view class="goods-section"><view class="address-tit">商品信息</view><block wx:for="{{dataList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><block><view class="g-item"><image class="product-img" src="{{v.goodsimage}}"></image><view class="right"><text class="title clamp">{{v.goodsname}}</text><view style="margin:5px 0;"><text class="spec uni-ellipsis">{{v.name}}</text></view><view class="price-box"><text class="price">{{"￥"+v.price}}</text><text class="number">{{"x"+v.numbers}}</text></view></view></view></block></block></view><block wx:if="{{distance!=''}}"><view class="uni-list-cell"><text class="cell-tip">运费说明</text><view class="cell-tit">{{distance}}</view></view></block><view data-event-opts="{{[['tap',[['getCompline',['$event']]]]]}}" class="uni-list-cell" bindtap="__e"><text class="cell-tip">选择优惠券</text><view style="display:flex;align-items:center;"><block wx:if="{{compline.mycouponid==''}}"><text style="margin-right:20rpx;color:#666666;">请选择</text></block><block wx:else><text style="margin-right:20rpx;color:#666666;">{{"您选择了"+compline.price+"元优惠券"}}</text></block><view class="uni-icon uni-icon-arrowright"></view></view></view><view class="uni-list-cell"><text class="cell-tip">备注</text><input class="input" type="text" value="{{remarks}}" placeholder="希望配送时间" data-event-opts="{{[['input',[['__set_model',['','remarks','$event',[]]]]]]}}" bindinput="__e"/></view><view class="uni-list-cell"><text class="cell-tip">商品金额</text><view class="cell-tit">{{"￥"+shopPrice}}</view></view><block wx:if="{{freight!=0}}"><view class="uni-list-cell"><text class="cell-tip">运费</text><view class="cell-tit">{{"￥"+freight}}</view></view></block><block wx:if="{{compline.price!=0}}"><view class="uni-list-cell"><text class="cell-tip">优惠券</text><view class="cell-tit">{{"￥"+compline.price}}</view></view></block><view class="uni-list-cell"><text class="cell-tip">共计</text><view class="cell-tit">{{'￥'+endPrice+''}}</view></view><view class="footer"><view class="price-content"><block wx:if="{{endPrice!=0}}"><text class="price-tip">￥</text></block><text class="price">{{endPrice}}</text></view><text class="submit" hover-class="btn-hover" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">提交订单</text></view></view>