progress {
  width: 60%;
  margin-right: 12rpx;
}
.noSecond {
  background-color: #929292;
  color: #FFFFFF;
  text-align: center;
}
page {
  background: #F6F6F6;
}
.btn {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 160rpx;
  line-height: 60rpx;
  height: 60rpx;
}
.content {
  box-sizing: border-box;
  margin-top: 10px;
}
.price {
  color: #FF0000;
  font-size: 28rpx;
  font-weight: bold;
}
.sku-name {
  color: #999999;
  font-size: 22rpx;
}
.uni-second-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding: 0 10px;
  margin-bottom: 10px;
}
.uni-second {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 20rpx;
  width: 100%;
  box-sizing: border-box;
  background-color: #FFFFFF;
  border-radius: 4rpx;
  margin-bottom: 20rpx;
}
.image-second {
  width: 155rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  box-sizing: border-box;
}
.uni-second-image {
  width: 155rpx;
  height: 155rpx;
  border-radius: 10rpx;
}
.uni-second-title {
  color: #333333;
  line-height: 1.5;
}
.uni-second-price {
  width: 75%;
  font-size: 28rpx;
  line-height: 1.5;
  padding-left: 15rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  position: relative;
}
.percent {
  position: absolute;
  top: 50%;
  left: 30%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  color: #FF0000;
  font-size: 22rpx;
}
.percentNumber {
  color: #999999;
  font-size: 22rpx;
}
.uni-space {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.uni-flex {
  position: relative;
}

